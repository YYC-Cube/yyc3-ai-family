{
  "name": "yyc3-family-pi",
  "private": true,
  "version": "0.34.0",
  "type": "module",
  "description": "YYC³ AI-Family - 人机协同 · 智爱同行。赛博朋克美学DevOps智能平台。",
  "author": "YYC³ Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/YanYuCloudCube/Family-π³"
  },
  "scripts": {
    "dev": "vite --mode development",
    "dev:1": "vite --mode development",
    "dev:2": "vite --mode staging --port 3134",
    "dev:3": "vite --mode production --port 3135",
    "dev:4": "vite --mode testing --port 3136",
    "dev:staging": "vite --mode staging --port 3134",
    "dev:testing": "vite --mode testing --port 3136",
    "server": "tsx src/server/index.ts",
    "server:dev": "tsx src/server/index.ts",
    "server:staging": "tsx src/server/index.ts",
    "server:testing": "tsx src/server/index.ts",
    "server:prod": "tsx src/server/index.ts",
    "dev:full": "concurrently \"pnpm run dev\" \"pnpm run server:dev\" --names \"frontend,backend\"",
    "start": "bash scripts/integrated-deploy.sh start",
    "start:all": "bash scripts/integrated-deploy.sh start",
    "start:backend": "cd backend && pnpm dev",
    "start:frontend": "vite --mode development",
    "stop": "bash scripts/integrated-deploy.sh stop",
    "stop:all": "bash scripts/integrated-deploy.sh stop",
    "status": "bash scripts/integrated-deploy.sh status",
    "health": "bash scripts/integrated-deploy.sh health",
    "build": "vite build",
    "build:staging": "vite build --mode staging",
    "build:testing": "vite build --mode testing",
    "preview": "vite preview --port 3135",
    "preview:staging": "vite preview --port 3134",
    "preview:testing": "vite preview --port 3136",
    "lint": "eslint . --config eslint.config.js",
    "lint:fix": "eslint . --config eslint.config.js --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,json}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "type-check": "tsc --noEmit",
    "ci": "pnpm run lint && pnpm run type-check && pnpm test"
  },
