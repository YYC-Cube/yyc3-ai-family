{
  "name": "@yyc3/bigmodel-sdk",
  "version": "1.0.0",
  "description": "YYC³ BigModel-Z.ai API SDK - 完整的 TypeScript/JavaScript 客户端封装",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"**/*.ts\"",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "yyc3",
    "bigmodel",
    "ai",
    "llm",
    "chatbot",
    "sdk",
    "api",
    "typescript",
    "mcp"
  ],
  "author": "YanYuCloudCube Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/YYC-Cube/yyc3-ai-family",
    "directory": "packages/bigmodel-sdk"
  },
  "bugs": {
    "url": "https://github.com/YYC-Cube/yyc3-ai-family/issues"
  },
  "homepage": "https://github.com/YYC-Cube/yyc3-ai-family#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0"
  },
  "files": [
    "dist",
    "core",
    "mcp",
    "hooks",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./core": {
      "import": "./dist/core/index.js",
      "types": "./dist/core/index.d.ts"
    },
    "./mcp": {
      "import": "./dist/mcp/index.js",
      "types": "./dist/mcp/index.d.ts"
    },
    "./hooks": {
      "import": "./dist/hooks/index.js",
      "types": "./dist/hooks/index.d.ts"
    }
  }
}
