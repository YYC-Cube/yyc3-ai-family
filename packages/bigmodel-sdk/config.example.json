{
  "bigmodel": {
    "apiKey": "your-api-key-here",
    "baseUrl": "https://open.bigmodel.cn/api/",
    "timeout": 30000,
    "model": "glm-4"
  },
  "openaiCompatible": {
    "apiKey": "your-api-key-here",
    "baseUrl": "https://open.bigmodel.cn/api/paas/v4/",
    "timeout": 30000,
    "model": "glm-4"
  },
  "mcp": {
    "fileSystem": {
      "paths": ["/Users/yourname", "/path/to/another/directory"]
    },
    "postgresql": {
      "connectionString": "postgresql://username:password@host:5432/database"
    },
    "braveSearch": {
      "apiKey": "your-brave-api-key-here"
    },
    "docker": {
      "host": "unix:///var/run/docker.sock"
    },
    "github": {
      "pat": "your-github-pat-here"
    },
    "yyc3cn": {
      "serverPath": "/path/to/yyc3-cn-mcp-server.js",
      "mode": "development",
      "version": "latest"
    }
  },
  "development": {
    "nodeEnv": "development",
    "logLevel": "info",
    "debug": false
  },
  "paths": {
    "projectRoot": "/path/to/your/project",
    "tempDir": "/tmp/bigmodel-sdk",
    "cacheDir": "/tmp/bigmodel-sdk/cache"
  }
}
